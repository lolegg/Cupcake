@{
    Layout = null;
}
<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />
    <title></title>
    <meta name="description" content="blank page" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />

    <!--Basic Styles-->
    <link href="~/Content/css/bootstrap-3.3.6.css" rel="stylesheet" />
    <!--Fonts-->
    <link href="~/Content/css/font-awesome.css" rel="stylesheet" />
    <link href="~/Content/css/typicons.css" rel="stylesheet" />
    <!--Reloadsoft styles-->
    <link href="~/Content/css/reload.css" rel="stylesheet" />
    <link href="~/Content/css/animate.min.css" rel="stylesheet" />
    <link id="skin-link" href="" rel="stylesheet" type="text/css" />

    <script src="~/Scripts/jquery-1.12.0.js"></script>
    <script src="~/Scripts/jquery-ui-1.11.4.js"></script>
    <script src="~/Scripts/skins.js"></script>
    <script src="~/Scripts/bootstrap-3.3.6.js"></script>
    @RenderSection("head", required: false)
</head>
<body>
    <div class="main-container container-fluid">
        <div class="page-container">
            <div class="page-content">
                <div class="page-body">
                    @RenderBody()
                </div>
            </div>
        </div>
    </div>

    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/jqueryvalreload")
    <script src="~/Scripts/form.js"></script>
    <script src="~/Content/js/datetime/bootstrap-datepicker.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
