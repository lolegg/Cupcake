@model Cupcake.Web.Models.ParameterModel
@using Cupcake.Web
@using Cupcake.Core.Domain
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Form.cshtml";
}

<div class="row">
    <div class="col-sm-12">
        <div class="well with-header">
            <div class="header bordered-info">
                参数信息
            </div>
            <div class="form-group">
                <div class="input-group">
                    @Html.LabelFor(model => model.Key, new { @class = "input-group-addon" })
                    @Html.EditorFor(m => m.Key)
                </div>
            </div>
            <div class="form-group">
                <div class="input-group">
                    @Html.LabelFor(model => model.Value, new { @class = "input-group-addon" })
                    @Html.EditorFor(m => m.Value)
                </div>
            </div>

        </div>
    </div>
</div>
